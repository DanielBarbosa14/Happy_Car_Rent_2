datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Loja {
  id    Int     @default(autoincrement()) @id
  name      String
  instructions String
  prepTime  String
  cookTime  String
  servings  Int
  image     String
  feito     Boolean
  carros CarroLoja[]
}

model Carro {
  id    Int     @default(autoincrement()) @id
  nome  String
  loja CarroLoja[]
}

model CarroLoja {
  loja     Loja @relation(fields: [lojaId], references: [id])
  lojaId   Int
  carro Carro @relation(fields: [carroId], references: [id])
  carroId Int
  quantidade  Int
  @@id([lojaId, carroId])
}
